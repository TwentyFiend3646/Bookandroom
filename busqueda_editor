            function accesoclave(){
                let entrada=document.getElementById('password').value;
                if(entrada==='Castellon5'){
                    document.getElementById('acceso_container').style.display="none";
                    document.getElementById('cuerpo').style.display="block";
                }
                if(entrada!='Castellon5'){
                    alert("Contraseña Erronea");
                }
            }
        </script>
