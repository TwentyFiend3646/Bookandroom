                <script>
                    function seleccion(value){
                        if(value==='a'){
                            document.getElementById("primera_seleccion").style.display="none";   
                            document.getElementById("cs").style.display="block";
                            document.getElementById("vl").style.display="none";
                        }
                        if(value==='b'){
                            document.getElementById("primera_seleccion").style.display="none";   
                            document.getElementById("vl").style.display="block";
                            document.getElementById("cs").style.display="none";
                        }
                            
                    }
                    function inicio(){
                        document.getElementById("primera_seleccion").style.display="block";
                        document.getElementById("cs").style.display="none";
                        document.getElementById("vl").style.display="none";
                    }
                    function switchmode2(value){
                        if(value==='a'){
                            document.getElementById("cs_list").style.display="flex";
                            document.getElementById("cs_map").style.display="none";
                            document.getElementById("List_op").style.background="#dfc6be";
                            document.getElementById("List_op").style.fontWeight="bold";
                            document.getElementById("Map_op").style.background="none";
                            document.getElementById("Map_op").style.fontWeight="normal";
                        }
                        if(value==='b'){
                            document.getElementById("cs_list").style.display="none";
                            document.getElementById("cs_map").style.display="flex";
                            document.getElementById("List_op").style.background="none";
                            document.getElementById("List_op").style.fontWeight="normal";
                            document.getElementById("Map_op").style.background="#dfc6be";
                            document.getElementById("Map_op").style.fontWeight="bold";
                        }
                    }
                    function switchmodevl2(value){
                        if(value==='a'){
                            document.getElementById("vl_list").style.display="flex";
                            document.getElementById("vl_map").style.display="none";
                            document.getElementById("List_op_vl").style.background="#dfc6be";
                            document.getElementById("List_op_vl").style.fontWeight="bold";
                            document.getElementById("Map_op_vl").style.background="none";
                            document.getElementById("Map_op_vl").style.fontWeight="normal";
                        }
                        if(value==='b'){
                            document.getElementById("vl_list").style.display="none";
                            document.getElementById("vl_map").style.display="flex";
                            document.getElementById("List_op_vl").style.background="none";
                            document.getElementById("List_op_vl").style.fontWeight="normal";
                            document.getElementById("Map_op_vl").style.background="#dfc6be";
                            document.getElementById("Map_op_vl").style.fontWeight="bold";
                        }
                    }
                </script>
